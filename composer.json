{
    "name": "parsidev/gravatar",
    "description": "Gravatar Package for Laravel 5.6",
    "authors": [
        {
            "name": "Mohammad Reza",
            "email": "info@parsidev.ir"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "6.0.*"
    },
    "autoload": {
        "psr-0": {
            "Parsidev\\Gravatar": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Parsidev\\Gravatar\\GravatarServiceProvider"
            ],
            "aliases": {
                "Gravatar": "Parsidev\\Gravatar\\Facades"
            }
        }
    },
    "minimum-stability": "stable"
}
